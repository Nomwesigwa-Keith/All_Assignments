drinks = {"coffee", "tea", "smoothie"}
print(drinks)

drinks.add("mocha")
drinks.add("shake")
print(drinks)

appliances = {"toaster", "blender", "heater", "dishwasher"}
print("heater" in appliances)

appliances.remove("blender")
print(appliances)

for item in appliances:
    print(item)

fruits = {"kiwi", "pineapple", "papaya", "melon"}
extra = ["plum", "peach"]
fruits.update(extra)

years = {1990, 2005, 2010}
people = {"Jane", "Ivan", "Kevin"}
merged = years.union(people)
print(merged)
