years_old = 25
note = "He is " + str(years_old) + " years young."
print(note)

line = "     Welcome,     Nigeria!     "
cleaned = line.replace(" ", "")
print(cleaned)

shout = line.upper()
print(shout)

tweaked = line.replace("N", "M")
print(tweaked)

statement = "We love Python coding"
print(statement[1:4])

quote = 'She said, "Coding is fun!"'
print(quote)
